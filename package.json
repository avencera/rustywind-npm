{
  "name": "rustywind",
  "version": "0.2.9",
  "description": "CLI for organizing Tailwind CSS classes",
  "main": "index.js",
  "scripts": {
    "install": "sh install.sh --git avencera/rustywind -f --to $(npm bin -g)",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./install.ts",
    "prod": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avencera/rustywind-npm.git"
  },
  "keywords": [
    "tailwind",
    "tailwind css",
    "headwind",
    "headwind cli"
  ],
  "postinstall": "npm run install --only=prod",
  "author": "Praveen Perera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avencera/rustywind-npm/issues"
  },
  "homepage": "https://github.com/avencera/rustywind-npm#readme",
  "devDependencies": {
    "@types/node": "^12.12.14",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "tar": "^5.0.5"
  }
}
